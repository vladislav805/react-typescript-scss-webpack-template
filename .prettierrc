{
  "trailingComma": "all",
  "tabWidth": 4,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "printWidth": 120,
  "singleQuote": true,
  "useTabs": false,
  "overrides": [
    {
      "files": ".js",
      "options": {
        "parser": "babel"
      }
    },
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"
      }
    }
  ]
}